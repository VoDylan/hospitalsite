{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "tsconfig-custom/tsconfig.base.json",
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "types": ["three"]
  },
  "include": [
    "src",
    "node_modules/@types"
  ]
}
